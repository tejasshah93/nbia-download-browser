{
  "name": "NBIA Download Manager",
  "key": "",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "31",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "https://researchweb.iiit.ac.in/*",
    "https://*.cancerimagingarchive.net/*",
    "http://54.68.32.118/*"
  ],
  "url_handlers": {
    "OpenDownloadManager": {
      "matches": [
        "http://researchweb.iiit.ac.in/~tejas.shah/gsoc15/demoApp/launch"
      ],
        "title": "Open Download Manager"
    }
  },
  "externally_connectable": {
    "matches": ["https://researchweb.iiit.ac.in/*"]
  }
}
